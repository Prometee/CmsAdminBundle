{% trans_default_domain "CmsAdminBundle" %}
<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title 'global.title'|trans %}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">

    {% block stylesheets %}
        {% block stylesheets_normalize %}
            {% stylesheets
                '@CmsAdminBundle/Resources/public/css/vendor/normalize/normalize.min.css'
            %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        {% block stylesheets_bootstrap %}
            {% stylesheets filter='cssrewrite'
            'bundles/cmsadmin/css/vendor/bootstrap/bootstrap.min.css'
            '@CmsAdminBundle/Resources/public/css/vendor/bootstrap/bootstrap-theme.min.css'
            %}{# need declaration with @ to be overridable in bootstrap-theme there is no "url(" #}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        {% block stylesheets_jqueryui %}
            {% stylesheets filter='cssrewrite'
            'bundles/cmsadmin/css/vendor/jquery-ui/jquery-ui-1.11.0.custom.min.css'
            '@CmsAdminBundle/Resources/public/css/vendor/jquery-ui/jquery-ui.structure.min.css'
            'bundles/cmsadmin/css/vendor/jquery-ui/jquery-ui.theme.min.css'
            %}{# need declaration with @ to be overridable in jquery-ui.structure there is no "url(" #}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        {% block stylesheets_default %}
            {% stylesheets filter='cssrewrite'
                '@CmsAdminBundle/Resources/public/css/admin.css'
                '@CmsAdminBundle/Resources/public/css/admin_custom.css'
            %}{# need declaration with @ to be overridable #}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
    {% endblock %}

    {% block javascripts_head %}
        {% javascripts
        '@CmsAdminBundle/Resources/public/js/vendor/modernizr/modernizr-2.6.2-respond-1.1.0.min.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}

</head>
<body{% block body_attributes '' %}>

{% block body '' %}

{% block javascripts_foot %}
    {% javascripts
    '@CmsAdminBundle/Resources/public/js/vendor/jquery/jquery-2.1.1.min.js'
    '@CmsAdminBundle/Resources/public/js/vendor/jquery/i18n/jquery.ui.datepicker-%locale%.js'
    '@CmsAdminBundle/Resources/public/js/vendor/jquery/jquery-ui-1.11.0.custom.min.js'
    '@CmsAdminBundle/Resources/public/js/vendor/bootstrap/bootstrap.min.js'
    '@CmsAdminBundle/Resources/public/js/collection.js'
    '@CmsAdminBundle/Resources/public/js/collection_modal.js'
    '@CmsAdminBundle/Resources/public/js/manage_tinymce.js'
    '@CmsAdminBundle/Resources/public/js/admin.js'
    '@CmsAdminBundle/Resources/public/js/admin_custom.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {{ tinymce_init() }}
{% endblock %}

</body>
</html>
