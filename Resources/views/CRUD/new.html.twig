{% extends 'CmsAdminBundle::layout.html.twig' %}

{% block content %}
	{% embed 'CmsAdminBundle:CRUD:form.html.twig' %}
		{% trans_default_domain 'CmsAdminBundle' %}
		{% block form_title ( translation_prefix ~ '.new.title' )|trans([], bundle_name) %}

		{% block form_actions_list %}
			<input type="submit" class="btn btn-primary" name="save_and_add" value="{{ 'global.form_action.save_and_add'|trans }}">&nbsp;
			{{ parent() }}
		{% endblock %}
	{% endembed %}
{% endblock %}